{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Channel",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "type": {
            "type": "string"
        },
        "verified": {
            "type": "boolean",
            "default": false
        },
        "created_at": {
            "type": "string",
            "format": "date-time"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time"
        }
    },
    "required": [
        "id",
        "description",
        "type",
        "verified",
        "created_at",
        "updated_at"
    ]
}
