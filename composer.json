{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "dedoc/scramble": "^0.12.8",
        "dotswan/filament-laravel-pulse": "dev-l12-compatibility",
        "filament/filament": "^3.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "jtmcc/atomic-deployments": "^1.0.0",
        "laravel-notification-channels/pagerduty": "dev-master",
        "laravel/fortify": "^1.7",
        "laravel/framework": "^12.0",
        "laravel/octane": "^2.8",
        "laravel/pulse": "^1.3",
        "laravel/sanctum": "^4.0",
        "livewire/livewire": "^3.5",
        "santigarcor/laratrust": "^8.4",
        "spatie/laravel-query-builder": "^6.0",
        "spatie/laravel-uptime-monitor": "^4.3",
        "spiral/roadrunner-cli": "^2.6.0",
        "spiral/roadrunner-http": "^3.3.0",
        "sqids/sqids": "^0.5.0",
        "stechstudio/filament-impersonate": "^3.15",
        "symfony/http-client": "^7.1",
        "symfony/mailgun-mailer": "^7.1"
    },
    "require-dev": {
        "ext-json": "*",
        "brianium/paratest": "^7.1",
        "fakerphp/faker": "^1.9.1",
        "jasonmccreary/laravel-test-assertions": "^2.0",
        "larastan/larastan": "^3.1",
        "laravel/pint": "^1.16",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "spatie/laravel-ignition": "^2.0",
        "swaggest/json-schema": "^0.12.43"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/J-T-McC/pagerduty.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/filament-laravel-pulse.git"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "spatie/laravel-uptime-monitor"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "lint": [
            "./vendor/bin/pint",
            "./vendor/bin/phpstan"
        ]
    }
}
